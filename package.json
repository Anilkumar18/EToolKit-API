{
  "name": "emate-toolkit",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production nodemon  src/index.js --exec babel-node --presets env,stage-2",
    "local-start": "cross-env NODE_ENV=local nodemon  src/index.js --exec babel-node --presets env,stage-2",
    "build": "cross-env NODE_ENV=production npm run test && npm run clean:dist && babel src -d dist && npm run copy-neededfiles",
    "local-build": "cross-env NODE_ENV=local npm run clean:dist && babel src -d dist && npm run copy-neededfiles",
    "clean:dist": "shx rm -rf ./dist",
    "serve": "cross-env NODE_ENV=production node dist/index.js",
    "copy-neededfiles": "shx mkdir dist/templates/ && shx cp -r src/templates/* dist/templates/ && shx mkdir dist/views/ && shx cp -r src/views/* dist/views/"
  },
